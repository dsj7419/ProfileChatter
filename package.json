{
    "name": "profilechatter",
    "version": "1.0.2",
    "description": "ProfileChatter – animated SVG chat bubbles for your GitHub README",
    "type": "module",
    "main": "src/main.js",
    "scripts": {
        "lint": "eslint .",
        "format": "prettier --write .",
        "generate": "node -e \"import('node:fs').then(fs=>{import('./src/main.js').then(m=>{fs.mkdirSync('dist',{recursive:true});fs.writeFileSync('dist/profile-chat.svg',m.generateChatSVG());console.log('✔ SVG written to dist/profile-chat.svg');});});\"",
        "build": "node src/build-profile.js",
        "preview": "npm run build && cmd /c start test.html",
        "config:dev": "npm run dev --prefix configurator-ui",
        "config:build": "npm run build --prefix configurator-ui"
    },
    "keywords": [
        "svg",
        "chat",
        "github",
        "profile",
        "animation"
    ],
    "author": "Dan Johnson",
    "license": "MIT",
    "engines": {
        "node": ">=20"
    },
    "dependencies": {
        "emoji-picker-element": "^1.26.3"
    },
    "devDependencies": {
        "dotenv": "^16.3.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "prettier": "^3.2.0"
    }
}
