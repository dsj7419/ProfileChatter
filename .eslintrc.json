      "browser": true,
      "node": true,
      "es2024": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
      "ecmaVersion": 2024,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn"
    },
    "ignorePatterns": ["dist/*"]
  }
  